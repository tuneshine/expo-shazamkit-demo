{
  "expo": {
    "name": "expo-shazamkit-demo",
    "slug": "expo-shazamkit-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuneshine.expo-shazamkit-demo",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Microphone required to run Shazam",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "07154e47-1d9e-4f6d-9a53-23567808abdb"
      }
    },
    "owner": "tobobo",
    "plugins": [
      [
        "expo-shazamkit",
        {
          "microphonePermission": "Microphone required to run Shazam"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.0"
          }
        }
      ]
    ]
  }
}
